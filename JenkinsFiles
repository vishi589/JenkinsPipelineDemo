pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        
         stages {
        stage('Build') {
            steps {
                echo 'Building'
            }
        }
        
         stages {
        stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
         }
         }
    }
}
